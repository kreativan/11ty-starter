<ul class="breadcrumbs">
  {% for item in breadcrumbs %}
    <li>
      {% if item.parentKey %}
        <span>{{ item.title }}</span>
      {% else %}
        <a href="{{ item.url }}">{{ item.title }}</a>
      {% endif %}
    </li>
  {% endfor %}
</nav>